<%= form_for(:user, :url => session_path(:user), id: "hidden-sign-in-field") do |f|  %>
    <%= f.email_field :email, type: "hidden", :class => "hidden-email-field"  %></dl>
    <%= f.password_field :password, type: "hidden", :class => "hidden-password-field"  %></dl>

  <button type="button" class="btn btn-primary btn-lg btn-block" id="demo-user-btn">Click Me To Be A Demo User!</button>
  <button type="button" class="btn btn-primary btn-lg btn-block" id="demo-lawyer-btn">Click Me To Be A Demo Lawyer!</button>
<% end %>